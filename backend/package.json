  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./dist/database/mikro-orm.config.js",
      "./src/database/mikro-orm.config.ts"
    ]
  },
  "scripts": {
    "clean": "rimraf dist tsconfig.build.tsbuildinfo",
    "build": "npm run clean && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npm run clean && nest start",
    "start:dev": "npm run clean && nest start --watch",
    "start:debug": "npm run clean && nest start --debug --watch",
    "start:prod": "npm run clean && nest start",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
